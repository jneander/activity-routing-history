{
  "name": "@jneander/activity-routing-history",
  "version": "1.0.1",
  "description": "Activity Routing with browser history",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "type": "module",
  "exports": {
    "import": "./dist/esm/index.js",
    "require": "./dist/cjs/index.js",
    "types": "./dist/types/index.d.ts"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc -p ./config/tsconfig.cjs.json",
    "build:esm": "tsc -p ./config/tsconfig.esm.json",
    "clean": "rimraf dist/*",
    "lint:eslint": "eslint .",
    "lint:eslint:fix": "eslint --fix .",
    "lint:prettier": "prettier --check .",
    "lint:prettier:fix": "prettier --write .",
    "prebuild": "npm run clean",
    "test": "mocha",
    "test:watch": "nodemon --exec 'npm run test'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jneander/activity-routing-history"
  },
  "keywords": [
    "routing"
  ],
  "author": "Jeremy Neander <jeremy.neander@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/jneander/activity-routing-history",
  "devDependencies": {
    "@babel/eslint-parser": "^7.19.1",
    "@jneander/activity-routing": "^2.0.3",
    "@jneander/prettier-config": "^1.0.0",
    "chai": "^4.3.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-mocha": "^10.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "history": "^5.0.0",
    "mocha": "^10.0.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.8.1",
    "sinon": "^15.0.0",
    "typescript": "^4.9.4"
  },
  "peerDependencies": {
    "@jneander/activity-routing": "^1.0.0 || ^2.0.0",
    "history": "^5.0.0"
  },
  "prettier": "@jneander/prettier-config"
}
