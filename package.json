{
  "name": "@jneander/activity-routing-history",
  "version": "1.0.1",
  "description": "Activity Routing with browser history",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "type": "module",
  "exports": {
    "import": "./dist/esm/index.js",
    "require": "./dist/cjs/index.js",
    "types": "./dist/types/index.d.ts"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc -p ./config/tsconfig.cjs.json",
    "build:esm": "tsc -p ./config/tsconfig.esm.json",
    "clean": "rimraf dist/*",
    "lint:commits": "commitlint --from $(git symbolic-ref --short refs/remotes/origin/HEAD) --to HEAD",
    "lint:eslint": "eslint .",
    "lint:eslint:fix": "eslint --fix .",
    "lint:prettier": "prettier --check .",
    "lint:prettier:fix": "prettier --write .",
    "prebuild": "npm run clean",
    "prepare": "[ -z $CI ] && git config --local core.hooksPath .githooks || true",
    "prepublishOnly": "npm run build",
    "test": "mocha",
    "test:types": "tsc --noEmit",
    "test:types:watch": "tsc --noEmit --watch",
    "test:watch": "nodemon -e ts --exec 'npm run test'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jneander/activity-routing-history"
  },
  "keywords": [
    "routing"
  ],
  "author": "Jeremy Neander <jeremy.neander@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/jneander/activity-routing-history",
  "devDependencies": {
    "@commitlint/cli": "^17.0.0",
    "@jneander/activity-routing": "^2.0.3",
    "@jneander/commitlint-config": "^1.0.1",
    "@jneander/prettier-config": "^1.0.0",
    "@jneander/semantic-release-config": "^1.0.1",
    "@types/chai": "^4.3.4",
    "@types/mocha": "^10.0.1",
    "@types/qs": "^6.9.7",
    "@types/sinon": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.46.1",
    "chai": "^4.3.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-mocha": "^10.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "history": "^5.0.0",
    "lint-staged": "^13.0.0",
    "mocha": "^10.0.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.8.1",
    "sinon": "^15.0.0",
    "ts-node": "^10.0.0",
    "typescript": "^4.9.4"
  },
  "peerDependencies": {
    "@jneander/activity-routing": "^1.0.0 || ^2.0.0",
    "history": "^5.0.0"
  },
  "commitlint": {
    "extends": "@jneander/commitlint-config"
  },
  "lint-staged": {
    "*": "prettier --check --ignore-unknown"
  },
  "prettier": "@jneander/prettier-config",
  "release": {
    "extends": [
      "@jneander/semantic-release-config"
    ]
  }
}
